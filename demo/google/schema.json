{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "user_id": {
            "type": "string",
            "transformation":"JStringVariant<{name}, 21>"
        },
        "gmap_id": {
            "type": "string",
            "transformation":"JStringVariant<{name}, 37>"
        },
        "rating": {
            "type": "integer",
            "transformation": "JNumber<uint8_t, {name}>"
        },
        "category": {
            "type": "string",
            "transformation":"JStringVariant<{name}, 470>"
        },
        "latitude": {
            "type": "number"
        },
        "longitude": {
            "type": "number"
        }
    },
    "required": [
